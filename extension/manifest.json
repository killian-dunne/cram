{
  "manifest_version": 3,
  "name": "Cram!",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cram!"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": [],
      "js": ["content-script.js"]
    }
  ]
}
